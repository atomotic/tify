{
  "name": "tify",
  "version": "0.28.1",
  "description": "A slim and mobile-friendly IIIF document viewer",
  "homepage": "https://tify.rocks/",
  "repository": {
    "type:": "git",
    "url": "https://github.com/tify-iiif-viewer/tify"
  },
  "bugs": {
    "url": "https://github.com/tify-iiif-viewer/tify/issues"
  },
  "keywords": [
    "iiif",
    "viewer",
    ":)"
  ],
  "license": "AGPL-3.0",
  "main": "dist/tify.js",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "mock-api": "node tests/iiif-api/server.js",
    "postinstall": "node build/create-icons.js",
    "preversion": "npm install-clean && npm run test",
    "preview": "vite preview",
    "test:unit": "vitest run --environment jsdom --root src/",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "version": "npm run build && git add dist"
  },
  "devDependencies": {
    "@mdi/js": "^7.2.96",
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-airbnb": "^7.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/test-utils": "^2.3.2",
    "canvas": "^2.11.0",
    "click-outside-vue3": "^4.0.1",
    "cross-spawn": "^7.0.3",
    "cypress": "^12.8.1",
    "eslint": "^8.36.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.10.0",
    "jsdom": "^21.1.1",
    "openseadragon": "^3.0.0",
    "sass": "^1.60.0",
    "start-server-and-test": "^2.0.0",
    "striptags": "^3.2.0",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.2.1",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-sass-glob-import": "^2.0.0",
    "vitest": "^0.29.7",
    "vitest-canvas-mock": "^0.2.2",
    "vue": "^3.2.47"
  },
  "engines": {
    "node": ">= 16",
    "npm": ">= 8"
  }
}
